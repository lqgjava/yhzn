	}

	public String getGtMen() {
		return gtMen;
	}

	public void setGtMen(String gtMen) {
		this.gtMen = gtMen;
	}

	public String getCustaomer() {
		return custaomer;
	}

	public void setCustaomer(String custaomer) {
