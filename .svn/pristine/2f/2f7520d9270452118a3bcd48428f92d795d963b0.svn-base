.table {
	width:100%;
	border-collapse: collapse;
	text-align: center;
	margin-top: 10px;
	margin-bottom: 10px
}

.table td, .table th {
	border: 1px solid #cad9ea;
	color: #666;
	height: 30px;
}

.table thead th {
	background-color: #CCE8EB;
	width: 16.6%;
}

.table tr:nth-child(odd) {
	background: #fff;
}

.table tr:nth-child(even) {
	background: #F5FAFA;
}