package com.yhzn.model.finance;

public class ReceivableModel {

	private String id;
	private String company ;//单位
	private String chargeMan;//负责人
	private String chargeTel;//电话
	private String financeMan ;//财务
	private String financeTel;//电话
	private String money;//总金额
	private String remark;//备注
	private String insertDate;//新增日期
	private String insertName;
	private String modifyDate;
	private String modifyName;
	private int delectFlag;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getCompany() {
		return company;
	}
	public void setCompany(String company) {
		this.company = company;
	}
	public String getChargeMan() {
		return chargeMan;
	}
	public void setChargeMan(String chargeMan) {
		this.chargeMan = chargeMan;
	}
	public String getChargeTel() {
		return chargeTel;
	}
	public void setChargeTel(String chargeTel) {
		this.chargeTel = chargeTel;
	}
	public String getFinanceMan() {
		return financeMan;
	}
	public void setFinanceMan(String financeMan) {
		this.financeMan = financeMan;
	}
	public String getFinanceTel() {
		return financeTel;
	}
	public void setFinanceTel(String financeTel) {
		this.financeTel = financeTel;
	}
	public String getMoney() {
		return money;
	}
	public void setMoney(String money) {
		this.money = money;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getInsertDate() {
		return insertDate;
	}
	public void setInsertDate(String insertDate) {
		this.insertDate = insertDate;
	}
	
	public String getInsertName() {
		return insertName;
	}
	public void setInsertName(String insertName) {
		this.insertName = insertName;
	}
	public String getModifyDate() {
		return modifyDate;
	}
	public void setModifyDate(String modifyDate) {
		this.modifyDate = modifyDate;
	}
	public String getModifyName() {
		return modifyName;
	}
	public void setModifyName(String modifyName) {
		this.modifyName = modifyName;
	}
	public int getDelectFlag() {
		return delectFlag;
	}
	public void setDelectFlag(int delectFlag) {
		this.delectFlag = delectFlag;
	}
	
	public ReceivableModel(String id, String company, String chargeMan, String chargeTel, String financeMan,
			String financeTel, String money, String remark, String insertDate, String insertName,
			String modifyDate, String modifyName, int delectFlag) {
		super();
		this.id = id;
		this.company = company;
		this.chargeMan = chargeMan;
		this.chargeTel = chargeTel;
		this.financeMan = financeMan;
		this.financeTel = financeTel;
		this.money = money;
		this.remark = remark;
		this.insertDate = insertDate;
		this.insertName = insertName;
		this.modifyDate = modifyDate;
		this.modifyName = modifyName;
		this.delectFlag = delectFlag;
	}
	public ReceivableModel() {
		super();
	}

	
}
